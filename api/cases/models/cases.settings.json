{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "cases",
  "info": {
    "name": "Cases"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "case": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "age": {
      "type": "integer",
      "min": 0
    },
    "gender": {
      "type": "string",
      "maxLength": 1,
      "minLength": 1,
      "required": true
    },
    "nationality": {
      "type": "string"
    },
    "dateOfLabConfirmation": {
      "type": "date"
    },
    "exposure": {
      "type": "string"
    },
    "facilityOfAdmission": {
      "type": "string"
    },
    "arrivalDate": {
      "type": "date"
    },
    "withCoMorbidity": {
      "type": "boolean"
    },
    "previousConditions": {
      "type": "string"
    },
    "symptoms": {
      "type": "string"
    },
    "resultDate": {
      "type": "date"
    },
    "travelHistory": {
      "type": "string"
    },
    "onsetOfSymptoms": {
      "type": "date"
    },
    "currentLocation": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "string",
      "required": true
    },
    "dateUpdatedAt": {
      "type": "string",
      "required": true
    },
    "barangay": {
      "model": "barangay"
    }
  }
}

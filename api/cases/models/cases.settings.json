{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "cases",
  "info": {
    "name": "Cases"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Case": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Age": {
      "type": "integer",
      "min": 0
    },
    "Gender": {
      "type": "string",
      "maxLength": 1,
      "minLength": 1,
      "required": true
    },
    "Nationality": {
      "type": "string"
    },
    "DateOfLabConfirmation": {
      "type": "date"
    },
    "Exposure": {
      "type": "string"
    },
    "FacilityOfAdmission": {
      "type": "string"
    },
    "ArrivalDate": {
      "type": "date"
    },
    "WithCoMorbidity": {
      "type": "boolean"
    },
    "PreviousConditions": {
      "type": "string"
    },
    "Symptoms": {
      "type": "string"
    },
    "ResultDate": {
      "type": "date"
    },
    "TravelHistory": {
      "type": "string"
    },
    "OnsetOfSymptoms": {
      "type": "date"
    },
    "CurrentLocation": {
      "type": "string",
      "required": true
    },
    "Status": {
      "type": "string",
      "required": true
    },
    "barangay": {
      "model": "barangay"
    }
  }
}
